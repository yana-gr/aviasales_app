    "@testing-library/user-event": "^13.5.0",
    "classnames": "^2.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rimraf": "^5.0.1",
    "sass": "^1.66.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix --color",
    "format": "prettier ./src/**/*.js --write",
    "clear": "rimraf ./dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
